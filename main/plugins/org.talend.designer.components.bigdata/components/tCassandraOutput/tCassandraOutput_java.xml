			NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="PORT"
			REPOSITORY_VALUE="PORT"
			REQUIRED="true"
			NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
