		<PARAMETER NAME="BROKER_LIST" FIELD="TEXT" NUM_ROW="30">
			<DEFAULT>"host1:port1,host2:port2,..."</DEFAULT>
		</PARAMETER>

		<!-- The broker list is only required for output components.
		<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="30">
			<DEFAULT><![CDATA[(for tKafkaOutput)]]></DEFAULT>
		</PARAMETER>-->

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
