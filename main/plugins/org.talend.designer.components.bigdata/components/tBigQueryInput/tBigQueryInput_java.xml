		<IMPORT NAME="google-api-services-bigquery-v2-rev5-1.5.0-beta.jar" MODULE="google-api-services-bigquery-v2-rev5-1.5.0-beta.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.google/lib/google-api-services-bigquery-v2-rev5-1.5.0-beta.jar" REQUIRED="true" />
		<IMPORT NAME="google-http-client-1.8.3-beta.jar" MODULE="google-http-client-1.8.3-beta.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.google/lib/google-http-client-1.8.3-beta.jar" REQUIRED="true" />
		<IMPORT NAME="google-oauth-client-1.8.0-beta.jar" MODULE="google-oauth-client-1.8.0-beta.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.google/lib/google-oauth-client-1.8.0-beta.jar" REQUIRED="true" />
		<IMPORT NAME="guava-10.0.1.jar" MODULE="guava-10.0.1.jar" UrlPath="platform:/plugin/org.talend.libraries.guava/lib/guava-10.0.1.jar" REQUIRED="true" />
		<IMPORT NAME="jackson-core-asl-1.9.1.jar" MODULE="jackson-core-asl-1.9.1.jar" UrlPath="platform:/plugin/org.talend.libraries.jackson/lib/jackson-core-asl-1.9.1.jar" REQUIRED="true" />
     </IMPORTS>
