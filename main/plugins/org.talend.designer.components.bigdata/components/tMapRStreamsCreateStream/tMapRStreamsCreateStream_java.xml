<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Internet/MapR Streams</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR NAME="PARTITION" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="MAPRSTREAMS" FIELD="HADOOP_DISTRIBUTION"
			NUM_ROW="10" REQUIRED="false" GROUP="VERSION" />

		<PARAMETER NAME="ZOOKEEPER_CONNECT" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"zk1:port1,zk2:port2,..."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STREAM_ACTION" FIELD="CLOSED_LIST" NUM_ROW="25">
			<ITEMS DEFAULT="CREATE_STREAM">
				<ITEM NAME="CREATE_STREAM" VALUE="CREATE_STREAM" />
				<ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS" />
				<ITEM NAME="ALTER_STREAM" VALUE="ALTER_STREAM" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="STREAM" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"/mystream"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STREAM_REPLICATION" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="31">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STREAM_PARTITION" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="32">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STREAM_RETENTION_CHECK" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="33">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STREAM_RETENTION" FIELD="TEXT"
			REQUIRED_IF="(STREAM_RETENTION_CHECK == 'true')" NUM_ROW="33"
			SHOW_IF="(STREAM_RETENTION_CHECK == 'true')">
			<DEFAULT>604800000</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ZOOKEEPER_SECURITY_IS_ENABLED" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="35">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STREAM_PROPERTIES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="40" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION />
	<RETURNS />
</COMPONENT>
