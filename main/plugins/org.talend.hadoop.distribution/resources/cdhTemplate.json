{
	"id": "CDH5.X",
	"name": "Cloudera CDH5.X template",
	"distribution": "CLOUDERA",
	"baseVersion":"2.6.0-mr1-cdh5.10.0",
	"topVersion":"",
	"dynamicVersion":"will be filled during runtime",
	"repository": "https://repository.cloudera.com/cloudera/cdh-releases-rcs",
	"properties": {
	    "servicePrefix": "org.talend.hadoop.distribution.component"
	},
	"services": [
		"{properties.servicePrefix}.HadoopComponent",
		"{properties.servicePrefix}.HDFSComponent",
		"{properties.servicePrefix}.HBaseComponent",
		"{properties.servicePrefix}.HCatalogComponent",
		"{properties.servicePrefix}.HiveComponent",
		"{properties.servicePrefix}.HiveOnSparkComponent",
		"{properties.servicePrefix}.ImpalaComponent",
		"{properties.servicePrefix}.MRComponent",
		"{properties.servicePrefix}.PigComponent",
		"{properties.servicePrefix}.SqoopComponent",
		"{properties.servicePrefix}.SparkBatchComponent",
		"{properties.servicePrefix}.SparkStreamingComponent"
	],
	"conditions": [
		{
			"id": "condition1",
			"operator": "==",
			"left": "a",
			"right": "b"
		},
		{
			"id": "condition2",
			"operator": "==",
			"left": "c",
			"right": "d"
		},
		{
			"id": "condition3",
			"operator": "AND",
			"left": "conditions.condition1",
			"right": "conditions.condition2"
		}
	],
	"modules": [
		{
			"id": "id1",
			"type": "STANDARD",
			"jarName": "talendConfig.jar",
			"mvnUri": ""
		},
		{
			"id": "hdfs.base.jar",
			"type": "BASE",
			"groupId": "org.apache.hadoop",
			"artifactId": "hadoop-client",
			"classifier": ""
		},
		{
			"id": "talendConfig.reference",
			"type": "REFERENCE",
			"jarName": "hadoop-hdfs-cdh5.10"
		}
	],
	"moduleGroups": [
		{
			"id": "module1",
			"description": "",
			"modules": [
				"id1",
				"hdfs.base.jar"
			],
			"conditions": [
				"conditions.condition1"
			]
		},
		{
			"id": "module2",
			"modules": [
				"talendConfig.reference",
				"moduleGroups.module1"
			],
			"conditions": [
				"conditions.condition1",
				"conditions.condition2"
			]
		}
	],
	"typeMap": [
		{
			"type": "HDFS",
			"moduleGroups": [
				{
					"moduleGroup": "module1",
					"conditions": [
						"conditions.condition1"
					]
				}
			]
		},
		{
			"type": "PIG",
			"moduleGroups": [
				{
					"moduleGroup": "module2",
					"conditions": [
						"conditions.condition1",
						"conditions.condition2"
					]
				}
			]
		}
	],
	"componentTypeMap": [
		{
			"type": "MAPREDUCE",
			"component": "tS3Input",
			"moduleGroups": [
				{
					"moduleGroup": "module2",
					"conditions": [
						"conditions.condition1",
						"conditions.condition2"
					]
				}
			]
		},
		{
			"type": "PIG",
			"component": "tPigStoreResult",
			"moduleGroups": [
				{
					"moduleGroup": "module2",
					"conditions": [
						"conditions.condition1",
						"conditions.condition2"
					]
				}
			]
		}
	],
	"classLoaders":[
	   {
	       "id":"",
	       "index":"HIVE:CLOUDERA:{dynamicVersion}:EMBEDDED",
	       "moduleGroup":""
	   },
	   {
	       "id":"",
           "index":"HIVE:CLOUDERA:{dynamicVersion}:STANDALONE",
           "moduleGroup":""
	   }
	]
}