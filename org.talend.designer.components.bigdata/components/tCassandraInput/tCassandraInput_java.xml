<COMPONENT>
	<HEADER
	    PLATEFORM="ALL"
	    SERIAL=""
	    VERSION="0.102"
	    STATUS="ALPHA"
	    COMPATIBILITY="ALL"
	    AUTHOR="Talend"
	    RELEASE_DATE="20120706A"
	    STARTABLE="true"
  	>
    	<SIGNATURE/>
  	</HEADER>
	<FAMILIES>
		<FAMILY>Databases/Cassandra</FAMILY>
		<FAMILY>Big Data/Cassandra</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
	    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER
			NAME="USE_EXISTING_CONNECTION"
			FIELD="CHECK"
			NUM_ROW="2"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="CONNECTION"
			FIELD="COMPONENT_LIST"
			FILTER="tCassandraConnection"
			NUM_ROW="2"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
		/>
		<PARAMETER 
            NAME="DB_VERSION" 
            FIELD="CLOSED_LIST" 
            NUM_ROW="3"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
            <ITEMS DEFAULT="CASSANDRA_1_1_2">
                <ITEM NAME="CASSANDRA_1_2_2" VALUE="CASSANDRA_1_2_2"/>
                <ITEM NAME="CASSANDRA_1_1_2" VALUE="CASSANDRA_1_1_2"/>
            </ITEMS>
        </PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="HOST"
			REQUIRED="true"
			NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="PORT"
			REQUIRED="true"
			NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>"9160"</DEFAULT>
		</PARAMETER>
		<!--
		<PARAMETER
			FIELD="TEXT"
			NAME="CLUSTER"
			NUM_ROW="6"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		-->
		<PARAMETER
			NAME="REQUIRED_AUTHENTICATION"
			FIELD="CHECK"
			NUM_ROW="10"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="USERNAME"
			REQUIRED="true"
			NUM_ROW="11"
			SHOW_IF="(REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="PASSWORD"
			REQUIRED="true"
			NUM_ROW="11"
			SHOW_IF="(REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="KEY_SPACE"
			REQUIRED="true"
			NUM_ROW="15"
			GROUP="KEYSPACE_CONFIGURATION"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="COLUMN_FAMILY"
			REQUIRED="true"
			NUM_ROW="16"
			GROUP="COLUMN_FAMILY_CONFIGURATION"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
            NAME="COLUMN_FAMILY_TYPE"
            FIELD="CLOSED_LIST"
            NUM_ROW="16"
            GROUP="COLUMN_FAMILY_CONFIGURATION"
        >
            <ITEMS DEFAULT="STANDARD">
                <ITEM NAME="STANDARD" VALUE="STANDARD" />
                <ITEM NAME="SUPER" VALUE="SUPER"/>
            </ITEMS>
        </PARAMETER>
		<PARAMETER
			FIELD="CHECK"
			NAME="INC_KEY"
			NUM_ROW="20"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="COLUMN_LIST"
			NAME="KEY_COLUMN"
			NUM_ROW="20"
			SHOW_IF="(INC_KEY == 'true')"
		/>
		<PARAMETER
            FIELD="CHECK"
            NAME="INC_SUPER_KEY"
            NUM_ROW="21"
            SHOW_IF="(COLUMN_FAMILY_TYPE == 'SUPER')"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="COLUMN_LIST"
            NAME="SUPER_KEY_COLUMN"
            NUM_ROW="21"
            SHOW_IF="(COLUMN_FAMILY_TYPE == 'SUPER') AND (INC_SUPER_KEY == 'true')"
        />
        <PARAMETER
        	FIELD="OPENED_LIST"
        	NAME="ROW_KEY_TYPE"
        	NUM_ROW="22"
        	REQUIRED="true"
			 >
    		<ITEMS DEFAULT="STRING">
    			<ITEM NAME="INTEGER" VALUE="Integer" />
    			<ITEM NAME="LONG" VALUE="Long" />
    			<ITEM NAME="DOUBLE" VALUE="Double" />
    			<ITEM NAME="FLOAT" VALUE="Float" />
    			<ITEM NAME="CHARACTER" VALUE="Character" />
    			<ITEM NAME="STRING" VALUE="String" />
    			<ITEM NAME="DATE" VALUE="java.util.Date" />
    			<ITEM NAME="BYTE_ARRAY" VALUE="byte[]" />
    			<ITEM NAME="BOOLEAN" VALUE="Boolean" />
    			<ITEM NAME="OBJECT" VALUE="Object" />
    			<ITEM NAME="SHORT" VALUE="Short" />
    		</ITEMS>
    	</PARAMETER>
    	<PARAMETER
        	FIELD="OPENED_LIST"
        	NAME="SUPER_COLUMN_TYPE"
        	NUM_ROW="22"
        	REQUIRED="true"
        	SHOW_IF="(COLUMN_FAMILY_TYPE == 'SUPER')">
    		<ITEMS DEFAULT="STRING">
    			<ITEM NAME="INTEGER" VALUE="Integer" />
    			<ITEM NAME="LONG" VALUE="Long" />
    			<ITEM NAME="DOUBLE" VALUE="Double" />
    			<ITEM NAME="FLOAT" VALUE="Float" />
    			<ITEM NAME="CHARACTER" VALUE="Character" />
    			<ITEM NAME="STRING" VALUE="String" />
    			<ITEM NAME="DATE" VALUE="java.util.Date" />
    			<ITEM NAME="BYTE_ARRAY" VALUE="byte[]" />
    			<ITEM NAME="BOOLEAN" VALUE="Boolean" />
    			<ITEM NAME="OBJECT" VALUE="Object" />
    			<ITEM NAME="SHORT" VALUE="Short" />
    		</ITEMS>
    	</PARAMETER>
        <PARAMETER
            FIELD="CHECK"
            NAME="SPECIFY_KEYS"
            NUM_ROW="23"
            GROUP="QUERY_CONFIGURATION"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="ROW_KEYS"
            REQUIRED="false"
            NUM_ROW="25"
            SHOW_IF="(SPECIFY_KEYS == 'true')"
            GROUP="QUERY_CONFIGURATION"
        >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		<PARAMETER
            FIELD="TEXT"
            NAME="KEY_START"
            REQUIRED="false"
            NUM_ROW="25"
            SHOW_IF="(SPECIFY_KEYS == 'false')"
            GROUP="QUERY_CONFIGURATION"
        >
            <DEFAULT></DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="KEY_END"
            REQUIRED="false"
            NUM_ROW="25"
            SHOW_IF="(SPECIFY_KEYS == 'false')"
            GROUP="QUERY_CONFIGURATION"
        >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		<PARAMETER
            FIELD="TEXT"
            NAME="KEY_LIMIT"
            REQUIRED="true"
            NUM_ROW="26"
            GROUP="QUERY_CONFIGURATION"
            SHOW_IF="(SPECIFY_KEYS == 'false')"
        >
            <DEFAULT>1000</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="CHECK"
            NAME="SPECIFY_COLUMNS"
            NUM_ROW="28"
            GROUP="QUERY_CONFIGURATION"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="COLUMNS"
            REQUIRED="false"
            NUM_ROW="29"
            SHOW_IF="(SPECIFY_COLUMNS == 'true')"
            GROUP="QUERY_CONFIGURATION"
        >
            <DEFAULT></DEFAULT>
        </PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="COLUMN_START"
			REQUIRED="false"
			NUM_ROW="30"
			SHOW_IF="(SPECIFY_COLUMNS == 'false')"
		    GROUP="QUERY_CONFIGURATION"
		>
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="COLUMN_END"
			REQUIRED="false"
			NUM_ROW="30"
			SHOW_IF="(SPECIFY_COLUMNS == 'false')"
            GROUP="QUERY_CONFIGURATION"
		>
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER
			FIELD="TEXT"
			NAME="COLUMN_LIMIT"
			REQUIRED="true"
			NUM_ROW="32"
			SHOW_IF="(SPECIFY_COLUMNS == 'false')"
            GROUP="QUERY_CONFIGURATION"
		>
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="SCHEMA_FLOW"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="35"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT MODULE="hector-core-1.0-5.jar" NAME="hector-core-1.0-5"
