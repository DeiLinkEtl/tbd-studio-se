			<IMPORT MODULE="apache-cassandra-1.1.2.jar" NAME="apache-cassandra-1.1.2"
				REQUIRED="true" />
			<IMPORT MODULE="apache-cassandra-thrift-1.1.2.jar" NAME="apache-cassandra-thrift-1.1.2"
				REQUIRED="true" />
			<IMPORT MODULE="slf4j-api-1.6.1.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.hive/lib/slf4j-api-1.6.1.jar" NAME="slf4j-api-1.6.1"
				REQUIRED="true" />
			<IMPORT MODULE="slf4j-log4j12-1.6.1.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.hive/lib/slf4j-log4j12-1.6.1.jar" NAME="slf4j-log4j12-1.6.1"
				REQUIRED="true" />
