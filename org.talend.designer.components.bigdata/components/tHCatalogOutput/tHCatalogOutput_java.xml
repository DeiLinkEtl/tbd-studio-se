		  NAME="ACTIVATE_LOG"
		  FIELD="CHECK"
		  NUM_ROW="10"
		  REQUIRED="true"
		>
		  <DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="STDOUT"
		  FIELD="DIRECTORY"
		  NUM_ROW="15"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		  SHOW_IF="ACTIVATE_LOG=='true'"
