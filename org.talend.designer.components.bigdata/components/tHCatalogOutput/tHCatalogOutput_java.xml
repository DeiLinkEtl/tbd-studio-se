				<ITEM NAME="HDP_1_2" VALUE="HDP_1_2" SHOW_IF="DISTRIBUTION=='HORTONWORKS'"/>
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="HADOOP_CUSTOM_JARS"  GROUP="VERSION" FIELD="HADOOP_JARS_DIALOG" NUM_ROW="10"  SHOW_IF="DISTRIBUTION=='CUSTOM'" REPOSITORY_VALUE="HADOOP_CUSTOM_JARS" >
        <DEFAULT />
        </PARAMETER>
        
				<PARAMETER
		  NAME="USE_KRB"
		  REPOSITORY_VALUE="USE_KRB"
		  FIELD="CHECK"
		  NUM_ROW="12"
		  REQUIRED="true"
		>
		  <DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NAMENODE_PRINCIPAL" FIELD="TEXT" REQUIRED="true" NUM_ROW="14" SHOW_IF="USE_KRB=='true'" GROUP="HDFS_CONFIGURATION">
			<DEFAULT>"nn/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FS_DEFAULT_NAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="15" GROUP="HDFS_CONFIGURATION">
		  	<DEFAULT>"hdfs://localhost:9000/"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILENAME" FIELD="BOTH_HADOOP" NUM_ROW="16" REQUIRED="true" GROUP="HDFS_CONFIGURATION">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILE_ACTION" FIELD="CLOSED_LIST" NUM_ROW="17" GROUP="HDFS_CONFIGURATION">
	      	<ITEMS DEFAULT="CREATE">
				<ITEM NAME="CREATE" VALUE="CREATE"/>
				<ITEM NAME="OVERWRITE" VALUE="OVERWRITE"/>
				<ITEM NAME="APPEND" VALUE="APPEND"/>
			</ITEMS>
	    </PARAMETER>
		<PARAMETER NAME="TEMPLETON_HOST" REPOSITORY_VALUE="TEMPLETON_HOST" FIELD="TEXT" NUM_ROW="20" REQUIRED="true" GROUP="TEMPLETON_CONFIGURATION">
			<DEFAULT>"localhost"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TEMPLETON_PORT" REPOSITORY_VALUE="TEMPLETON_PORT" FIELD="TEXT" NUM_ROW="20" REQUIRED="true" GROUP="TEMPLETON_CONFIGURATION">
			<DEFAULT>"50111"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="KRB_PRINC" REPOSITORY_VALUE="KRB_PRINC" FIELD="TEXT" NUM_ROW="22" REQUIRED="true" GROUP="TEMPLETON_CONFIGURATION" SHOW_IF="USE_KRB=='true'">
			<DEFAULT>"HTTP/__hostname__"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="KRB_REALM" REPOSITORY_VALUE="KRB_REALM" FIELD="TEXT" NUM_ROW="22" REQUIRED="true" GROUP="TEMPLETON_CONFIGURATION" SHOW_IF="USE_KRB=='true'">
			<DEFAULT>"EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="DATABASE_NAME"
		  REPOSITORY_VALUE="DATABASE_NAME"
		  FIELD="TEXT"
		  NUM_ROW="30"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		>
		  <DEFAULT>"default"</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="TABLE_NAME"
		  FIELD="TEXT"
		  NUM_ROW="40"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		>
		  <DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="PARTITION_NAME"
		  FIELD="TEXT"
		  NUM_ROW="50"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		>
		  <DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="USERNAME"
		  REPOSITORY_VALUE="USERNAME"
		  FIELD="TEXT"
		  NUM_ROW="60"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		>
		  <DEFAULT>"hdp"</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="FILE_LOCATION"
		  FIELD="TEXT"
		  NUM_ROW="70"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		>
		  <DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
		  NAME="DIE_ON_ERROR"
		  FIELD="CHECK"
		  NUM_ROW="90"
		  REQUIRED="true"
		>
		  <DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
	   <PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="1" REQUIRED="true">
            <DEFAULT>"\n"</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="1" REQUIRED="true">
