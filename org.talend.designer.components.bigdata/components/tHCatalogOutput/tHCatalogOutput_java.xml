		</PARAMETER>
		<PARAMETER
		  NAME="STDERR"
		  FIELD="DIRECTORY"
		  NUM_ROW="16"
		  REQUIRED="true"
		  GROUP="HCAT_CONFIGURATION"
		  SHOW_IF="ACTIVATE_LOG=='true'"
