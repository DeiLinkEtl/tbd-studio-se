<COMPONENT>

  	<HEADER
	    PLATEFORM="ALL"
	    SERIAL=""
	    VERSION="0.102"
	    STATUS="ALPHA"
	
	    COMPATIBILITY="ALL"
	    AUTHOR="Talend"
	    RELEASE_DATE="20130903A"
	    STARTABLE="true"
  	>
    	<SIGNATURE/>
  	</HEADER>

  	<FAMILIES>
    	<FAMILY>Big Data/Google Storage</FAMILY>
  	</FAMILIES>

  	<DOCUMENTATION>
    	<URL/>
  	</DOCUMENTATION>

  	<CONNECTORS>
	   	<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
	    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1"/>
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
  	</CONNECTORS>

    <PARAMETERS>
    
		<PARAMETER 
	    	NAME="USE_EXISTING_CONNECTION" 
	    	FIELD="CHECK"
	        NUM_ROW="10"  
	        GROUP="CONNECTION_CONFIG"
	 	>
	        	<DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER 
		    NAME="CONNECTION" 
		    FIELD="COMPONENT_LIST" 
		    FILTER="tGSConnection"
			NUM_ROW="10" 
		   	SHOW_IF="USE_EXISTING_CONNECTION == 'true'" 
		   	GROUP="CONNECTION_CONFIG"
	  	/>
	
	    <PARAMETER
	      	NAME="ACCESS_KEY"
	      	FIELD="TEXT"
	      	NUM_ROW="20"
	      	REQUIRED="true"
	      	GROUP="CONNECTION_CONFIG"
	      	SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	    >
	      <DEFAULT>""</DEFAULT>
	    </PARAMETER>
	
	    <PARAMETER
	      	NAME="SECRET_KEY"
	      	FIELD="TEXT"
	     	NUM_ROW="20"
	      	REQUIRED="true"
	      	GROUP="CONNECTION_CONFIG"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	    >
	        <DEFAULT>""</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER
	      	NAME="OBJECTS_PREFIX"
	      	FIELD="TEXT"
	      	NUM_ROW="35"
	      	REQUIRED="false"
	      	SHOW_IF="(GETLIST == 'false')"
	      	GROUP="QUERY_CONFIG"
	    >
	    </PARAMETER>
	    
	    <PARAMETER
	      	NAME="DELIMITER"
	      	FIELD="TEXT"
	      	NUM_ROW="35"
	      	REQUIRED="false"
	      	SHOW_IF="(GETLIST == 'false')"
	      	GROUP="QUERY_CONFIG"
	    >
	    </PARAMETER>
	    
	    <PARAMETER
	      	NAME="USE_KEYS_LIST"
	      	FIELD="CHECK"
	      	NUM_ROW="45"
	      	REQUIRED="true"
	      	SHOW_IF="(GETLIST == 'false')"
	      	GROUP="QUERY_CONFIG"
	    >
	        <DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER 
			NAME="KEYS"
			FIELD="TABLE"
			NUM_ROW="50"
			REQUIRED_IF="(USE_KEYS_LIST == 'true') AND (GETLIST == 'false')"
			NB_LINES="5"
			SHOW_IF="(USE_KEYS_LIST == 'true') AND (GETLIST == 'false')"
			GROUP="QUERY_CONFIG"
		>
	        <ITEMS>
	        	<ITEM NAME="BUCKET_NAME" VALUE="&quot;&quot;" />
	            <ITEM NAME="KEY" VALUE="&quot;&quot;" />
	            <ITEM NAME="NEW_NAME" VALUE="&quot;&quot;" />
	        </ITEMS>
	    </PARAMETER>
	    
	    <PARAMETER 
		    NAME="SPECIFY_PROJECT" 
		    FIELD="CHECK"
	        NUM_ROW="55"  
	        SHOW_IF="(GETLIST == 'false') AND (USE_KEYS_LIST == 'false')"
	        GROUP="QUERY_CONFIG"
  		>
        	<DEFAULT>false</DEFAULT>
    	</PARAMETER>
    	
     	<PARAMETER
	      	NAME="PROJECT_ID"
	      	FIELD="TEXT"
	      	NUM_ROW="55"
	     	REQUIRED_IF="(SPECIFY_PROJECT == 'true') AND (GETLIST == 'false') AND (USE_KEYS_LIST == 'false')"
	      	SHOW_IF="(SPECIFY_PROJECT == 'true') AND (GETLIST == 'false') AND (USE_KEYS_LIST == 'false')"
	      	GROUP="QUERY_CONFIG"
    	>
        	<DEFAULT>""</DEFAULT>
    	</PARAMETER>
	    
	    <PARAMETER
	      	NAME="GETLIST"
	      	FIELD="CHECK"
	      	NUM_ROW="60"
	      	REQUIRED="true"
	      	SHOW_IF="(USE_KEYS_LIST == 'false')"
	      	GROUP="QUERY_CONFIG"
	    >
	        <DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER 
			NAME="BUCKETS"
			FIELD="TABLE"
			NUM_ROW="65"
			REQUIRED_IF="(GETLIST == 'true')"
			NB_LINES="5"
			SHOW_IF="(GETLIST == 'true')"
			GROUP="QUERY_CONFIG"
		>
	        <ITEMS>
	            <ITEM NAME="BUCKET_NAME" VALUE="&quot;&quot;" />
	            <ITEM NAME="OBJECT_PREFIX" VALUE="&quot;&quot;" />
	            <ITEM NAME="OBJECT_DELIMITER" VALUE="&quot;&quot;" />
	        </ITEMS>
	    </PARAMETER>
	    
	    <PARAMETER 
      		NAME="DIRECTORY" 
      		FIELD="DIRECTORY" 
      		REQUIRED="true"
	      	NUM_ROW="70"
      	>
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out/"</DEFAULT>
		</PARAMETER>
	    
		<PARAMETER
		    NAME="DIE_ON_ERROR"
		    FIELD="CHECK"
		    NUM_ROW="90"
		>
		    <DEFAULT>false</DEFAULT>
		</PARAMETER>
    
   	</PARAMETERS>

    <ADVANCED_PARAMETERS/>

   	<CODEGENERATION>
	    <IMPORTS>
	      	<IMPORT NAME="jets3t-0.9.0" MODULE="jets3t-0.9.0.jar" REQUIRED="true" />
			<IMPORT NAME="commons-logging-1.1.1" MODULE="commons-logging-1.1.1.jar" UrlPath="platform:/base/plugins/org.apache.commons.logging_1.1.1.v201101211721.jar" REQUIRED="true" />
			<IMPORT NAME="httpclient-4.1.2" MODULE="httpclient-4.1.2.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpclient-4.1.2.jar" REQUIRED="true" />
			<IMPORT NAME="httpcore-4.1.2" MODULE="httpcore-4.1.2.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpcore-4.1.2.jar" REQUIRED="true" />
			<IMPORT NAME="commons-codec-1.4" MODULE="commons-codec-1.4.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-codec-1.4.jar" REQUIRED="true" />
	    </IMPORTS>
   	</CODEGENERATION>

    <RETURNS>
        <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
    </RETURNS>
    
</COMPONENT>
