<COMPONENT>
	<HEADER
    	PLATEFORM="ALL"
       	SERIAL=""
      	VERSION="0.101"
       	STATUS="ALPHA"
         
		COMPATIBILITY="ALL"
       	AUTHOR="Talend"
 		RELEASE_DATE="20070210A"
    	STARTABLE="true"
  		>
    	<SIGNATURE></SIGNATURE>
  	</HEADER>

  	<FAMILIES>
    	<FAMILY>File/Hadoop</FAMILY>
		<FAMILY>Big Data/HDFS</FAMILY>
  	</FAMILIES>

  	<DOCUMENTATION>
    	<URL/>
