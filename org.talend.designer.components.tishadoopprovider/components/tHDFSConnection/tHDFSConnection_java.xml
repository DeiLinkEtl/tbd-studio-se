  	<CONNECTORS>
	    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
	    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
  	</CONNECTORS>

  	<PARAMETERS>
		<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="10" REQUIRED="true">
			<ITEMS DEFAULT="HDP_1_0">
				<ITEM NAME="HDP_1_0" VALUE="hadoop-core-1.0.0.jar;commons-logging-1.1.1.jar;commons-configuration-1.6.jar;commons-lang-2.4.jar;hdp-dummy.jar" />
				<ITEM NAME="APACHE_1_0_0" VALUE="hadoop-core-1.0.0.jar;commons-logging-1.1.1.jar;commons-configuration-1.6.jar;commons-lang-2.4.jar" />
				<ITEM NAME="APACHE_0_20_204" VALUE="hadoop-core-0.20.204.0.jar;commons-logging-1.1.1.jar;commons-configuration-1.6.jar;commons-lang-2.4.jar" />
				<ITEM NAME="Cloudera_0.20_CDH3U1" VALUE="hadoop-0.20.2-cdh3u1-core.jar;commons-logging-1.0.4.jar" />
				<ITEM NAME="APACHE_0_20_2" VALUE="hadoop-0.20.2-core.jar;commons-logging-1.0.4.jar" />
				<ITEM NAME="MapR" VALUE="hadoop-0.20.2-dev-core.jar;commons-logging-1.0.4.jar;maprfs-0.1.jar;zookeeper-3.3.2.jar" />
			</ITEMS>
	   	</PARAMETER>
	  	<PARAMETER NAME="FS_DEFAULT_NAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="20">
	    	<DEFAULT IF="(DB_VERSION != 'MapR')">"hdfs://localhost:9000/"</DEFAULT>
			<DEFAULT IF="(DB_VERSION == 'MapR')">"maprfs:///"</DEFAULT>
	 	</PARAMETER>
	  
	  	<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="30">
      		<DEFAULT>"anonymous"</DEFAULT>
      	</PARAMETER>
      	<PARAMETER NAME="GROUP" FIELD="TEXT" REQUIRED="true" NUM_ROW="30" SHOW_IF="(DB_VERSION == 'APACHE_0_20_2') or (DB_VERSION == 'MapR')">
	      	<DEFAULT>"supergroup"</DEFAULT>
      	</PARAMETER>
    
	  	<PARAMETER NAME="HADOOP_ADVANCED_PROPERTIES" FIELD="TABLE" REQUIRED="false" NUM_ROW="40" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
			</ITEMS>
	   </PARAMETER> 
  	</PARAMETERS>
   
   
   	<ADVANCED_PARAMETERS>
 
