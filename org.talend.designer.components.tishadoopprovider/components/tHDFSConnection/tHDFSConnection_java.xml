	        <IMPORT NAME="apache-hadoop-0-20-2" MODULE="hadoop-0.20.2-core.jar" REQUIRED="false" />
	        
	        <IMPORT NAME="maprfs-0.1" MODULE="maprfs-0.1.jar" REQUIRED="false" />
	        <IMPORT NAME="zookeeper-3.3.2" MODULE="zookeeper-3.3.2.jar" REQUIRED="false" />
