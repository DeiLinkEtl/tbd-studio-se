  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:HIVE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER> 
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tHiveConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />     
	<PARAMETER NAME="DISTRIBUTION" FIELD="CLOSED_LIST" NUM_ROW="25" REQUIRED="true" GROUP="VERSION" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<ITEMS DEFAULT="HORTONWORKS">
			<ITEM NAME="HORTONWORKS" VALUE="HORTONWORKS"/>
			<ITEM NAME="APACHE" VALUE="APACHE"/>
			<ITEM NAME="CLOUDERA" VALUE="CLOUDERA"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="HIVE_VERSION" FIELD="CLOSED_LIST" NUM_ROW="25" REQUIRED="true" GROUP="VERSION" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<ITEMS DEFAULT="HDP_1_0">
			<ITEM NAME="HDP_1_0" VALUE="HDP_1_0" SHOW_IF="DISTRIBUTION=='HORTONWORKS'"/>
			<ITEM NAME="APACHE_0_20_203" VALUE="APACHE_0_20_203" SHOW_IF="DISTRIBUTION=='APACHE'"/>
			<ITEM NAME="APACHE_1_0_0" VALUE="APACHE_1_0_0" SHOW_IF="DISTRIBUTION=='APACHE'" />
			<ITEM NAME="Cloudera_CDH4" VALUE="Cloudera_CDH4" SHOW_IF="DISTRIBUTION=='CLOUDERA'" />
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="CONNECTION_MODE" FIELD="CLOSED_LIST" NUM_ROW="26" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION=='false'">
		<ITEMS DEFAULT="EMBEDDED">
			<ITEM NAME="EMBEDDED" VALUE="EMBEDDED" NOT_SHOW_IF="HIVE_VERSION=='APACHE_0_20_203'"/>
			<ITEM NAME="STANDALONE" VALUE="STANDALONE" NOT_SHOW_IF="HIVE_VERSION=='HDP_1_0'"/>
		</ITEMS>
	</PARAMETER>
	
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"10000"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND CONNECTION_MODE=='STANDALONE')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Hive</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="60"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
	<PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="65"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="70"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="GUESS_SCHEMA"
      FIELD="GUESS_SCHEMA"
      NUM_ROW="70"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="90"
    >
      <DEFAULT>"select id, name from employee"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
